{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../../dist/out-tsc",
    "types": [
      "node"
    ],
    "lib": [
      "es2015",
      "esnext"
    ],
    "emitDecoratorMetadata": true,
    "target": "es5",
    "module": "commonjs"
  },
  "exclude": [
    "**/*.spec.ts"
  ],
  "include": [
    "**/*.ts"
  ]
}
