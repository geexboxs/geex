le {\n  static forRoot(entities = [], options?) {\n    const providers = createDatabaseProviders(options, entities);\n    return {\n      module: DatabaseModule,\n      providers: providers,\n      exports: providers,\n    };\n  }\n}"),r.Hb(),r.Hb(),r.Ib(313,"blockquote",12),r.Ib(314,"strong"),r.lc(315,"Hint"),r.Hb(),r.lc(316," The "),r.Ib(317,"code"),r.lc(318,"forRoot()"),r.Hb(),r.lc(319," method may return a dynamic module either synchronously or asynchronously (i.e., via a "),r.Ib(320,"code"),r.lc(321,"Promise"),r.Hb(),r.lc(322,").\n"),r.Hb(),r.Ib(323,"p"),r.lc(324,"This module defines the "),r.Ib(325,"code"),r.lc(326,"Connection"),r.Hb(),r.lc(327," provider by default (in the "),r.Ib(328,"code"),r.lc(329,"@Module()"),r.Hb(),r.lc(330," decorator metadata), but additionally - depending on the "),r.Ib(331,"code"),r.lc(332,"entities"),r.Hb(),r.lc(333," and "),r.Ib(334,"code"),r.lc(335,"options"),r.Hb(),r.lc(336," objects passed into the "),r.Ib(337,"code"),r.lc(338,"forRoot()"),r.Hb(),r.lc(339," method - exposes a collection of providers, for example, repositories. Note that the properties returned by the dynamic module "),r.Ib(340,"strong"),r.lc(341,"extend"),r.Hb(),r.lc(342," (rather than override) the base module metadata defined in the "),r.Ib(343,"code"),r.lc(344,"@Module()"),r.Hb(),r.lc(345," decorator. That's how both the statically declared "),r.Ib(346,"code"),r.lc(347,"Connection"),r.Hb(),r.lc(348," provider "),r.Ib(349,"strong"),r.lc(350,"and"),r.Hb(),r.lc(351," the dynamically generated repository providers are exported from the module."),r.Hb(),r.Ib(352,"p"),r.lc(353,"If you want to register a dynamic module in the global scope, set the "),r.Ib(354,"code"),r.lc(355,"global"),r.Hb(),r.lc(356," property to "),r.Ib(357,"code"),r.lc(358,"true"),r.Hb(),r.lc(359,"."),r.Hb(),r.Ib(360,"pre"),r.Ib(361,"code",11),r.lc(362,"\n{\n  global: true,\n  module: DatabaseModule,\n  providers: providers,\n  exports: providers,\n}"),r.Hb(),r.Hb(),r.Ib(363,"blockquote",29),r.