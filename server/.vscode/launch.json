    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "server:Debug",
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/ts-node-dev",
            "args": [
                "-C",
                "ttypescript",
                "${workspaceRoot}/index"
            ],
            "restart": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "server:Run",
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/ts-node",
            "args": [
                "-C",
                "ttypescript",
                "${workspaceRoot}/index"
            ],
        }
    ]
}
