{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-classes-per-file": false,
        "object-literal-sort-keys": false,
        "max-line-length": {
            "options": {
                "limit": 200
            }
        },
        "no-console": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "utils/array.extension.ts"
        ]
    }
}
