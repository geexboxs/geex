{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-classes-per-file": false,
        "no-string-throw": true,
        "no-string-literal": false,
        "object-literal-sort-keys": false,
        "arrow-parens": false,
        "max-line-length": {
            "options": {
                "limit": 200
            }
        },
        "space-before-function-paren": false,
        "no-console": false,
        "ordered-imports": false,
        "align": false,
        "member-access": false,
        "ban-types": false,
        "prefer-const": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "utils/array.extension.ts",
            ".*/**/*"
        ]
    }
}
